<h1>Doctor <%= @doctor.name %></h1>

<ul>
  <% @doctor.appointments.each do |appt|%>
    <li><%= appt.human_date %> with <%= link_to appt.doctor.name, doctor_path(appt.doctor) %></li>
  <% end %>
</ul>

<ul>
  <% @doctor.patients.each do |patient| %>
    <li>
      Name: <%= link_to patient.name, patient_path(patient) %> Number of Appointments: <%= patient.appointment_count %>
    </li>
  <% end %>
</ul>
